{
  "manifest_version": 3,
  "name": "Tab Manager",
  "version": "1.0.0",
  "description": "Manage and organize your tabs efficiently",
  "permissions": ["tabs", "windows"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "tabs.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
